{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["ES2020"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "allowJs": true,
    "outDir": "lib",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "strictNullChecks": false
  },
  "include": ["src/**/*","src/**/*.json"]
}
